<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ecust.auth.mapper.AuthorityMapper">
    <select id="getAuthority" resultType="com.ecust.base.entity.Authority">
        select * from library.authority where id in
        <foreach collection="ids" item="id" separator="," open="(" close=")">
            #{id}
        </foreach>
    </select>

    <select id="getAuthorityNot" resultType="com.ecust.base.entity.Authority">
        select * from library.authority where id not in
        <foreach collection="ids" item="id" separator="," open="(" close=")">
            #{id}
        </foreach>
    </select>
</mapper>